{
    "name": "yobi supporter",
    "version": "0.1.1",
    "homepage_url": "https://github.com/khongchi/yobi-supporter",
    "description": "help you to use yobi.",
    "content_scripts": [
        {
            "matches": ["url of your yobi project"],
            "css": ["basic.css"],
            "js": [
                "jquery.js",
                "contentscript.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "yobi-logo.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "activeTab"
    ],
    "manifest_version": 2
}
